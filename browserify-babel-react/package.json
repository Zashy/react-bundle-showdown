{
  "name": "browserify-react",
  "version": "1.0.0",
  "scripts": {
    "dev": "browserify index.js > bundle.js && browser-sync start",
    "test": "standard --fix"
  },
  "dependencies": {
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.5",
    "browserify": "^13.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "standard": "^8.6.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ]
  },
  "browser-sync": {
    "server": true
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
